{
  "name": "@burnt-labs/xion-types",
  "version": "16.0.0",
  "description": "TypeScript definitions for Xion Protobuf files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run generate-types && npm run compile",
    "clean": "rm -rf dist",
    "generate-types": "../../scripts/proto-gen.sh --ts",
    "compile": "tsc --project tsconfig.json && npm run copy-files",
    "copy-files": "cp -r types/* dist/ && cp index.js dist/",
    "test": "npx jest",
    "test:ci": "npx jest --ci --passWithNoTests",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.14.1",
    "browser-headers": "^0.4.1",
    "google-protobuf": "^3.17.3",
    "long": "^4.0.0",
    "protobufjs": "~6.11.2"
  },
  "devDependencies": {
    "prettier": "^2.3.0",
    "ts-proto": "^1.82.5",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/burnt-labs/xion-types.git"
  },
  "keywords": [
    "xion",
    "blockchain",
    "typescript",
    "protobuf",
    "cosmos",
    "ibc"
  ]
}
